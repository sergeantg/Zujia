<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.zujia.android.zujia.activity.HouseDetailsActivity">

    <ImageSwitcher
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageSwitcher" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textView16" />

    <LinearLayout
        android:orientation="vertical"
        style="@style/MainLayoutStyle">

        <RelativeLayout style="@style/NoneMarginItemStyle">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView5"
                style="@style/ItemLeftIconStyle"
                android:src="@drawable/location_icon" />

            <TextView
                style="@style/ItemTextStyle"
                android:text="南开区八里台"
                android:layout_toRightOf="@+id/imageView5" />

            <ImageView
                style="@style/ItemRightIconStyle"
                android:src="@drawable/mail_arrow" />
        </RelativeLayout>

    </LinearLayout>
    <TableLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_margin="10dp">

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            style="@style/ItemStyle">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txtV_house_detail_type"
                android:layout_gravity="right"
                style="@style/ItemNameFont"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/textView19"
                android:layout_weight="1"
                style="@style/ItemContentFont"/>
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            style="@style/ItemStyle">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txtV_house_detail_rent"
                android:layout_gravity="right"
                style="@style/ItemNameFont"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/textView21"
                android:layout_weight="1"
                style="@style/ItemContentFont"/>
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            style="@style/ItemStyle">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txtV_house_detail_floor"
                android:layout_gravity="right"
                style="@style/ItemNameFont"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/textView23"
                android:layout_weight="1"
                style="@style/ItemContentFont"/>
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            style="@style/ItemStyle">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txtV_house_detail_date"
                android:layout_gravity="right"
                style="@style/ItemNameFont"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/textView25"
                android:layout_weight="1"
                style="@style/ItemContentFont"/>
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            style="@style/ItemStyle">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txtV_house_detail_decoration"
                android:layout_gravity="right"
                style="@style/ItemNameFont"/>

            <TextView
